{
  "name": "@leandigital/lean-intel",
  "version": "1.2.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "AI-powered documentation generation and code analyzer CLI",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "lean-intel": "./dist/src/index.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "NOTICE"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "dev": "tsx src/index.ts",
    "watch": "tsc --watch",
    "relink": "npm unlink -g @leandigital/lean-intel 2>/dev/null || true && npm run build && npm link",
    "package": "npm run build && npm pack",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "documentation",
    "due-diligence",
    "ai",
    "claude",
    "analysis",
    "security",
    "code-quality"
  ],
  "author": "Kirill Ulyanov",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/leandigital/lean-intel.git"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "overrides": {
    "test-exclude": "^7.0.1"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.78.0",
    "@google/generative-ai": "^0.24.1",
    "@octokit/rest": "^22.0.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "conf": "^15.1.0",
    "fs-extra": "^11.3.3",
    "glob": "^13.0.6",
    "highlight.js": "^11.11.1",
    "inquirer": "^13.2.2",
    "jsonrepair": "^3.13.2",
    "marked": "^17.0.1",
    "openai": "^6.21.0",
    "ora": "^8.1.0",
    "puppeteer": "^24.37.2",
    "simple-git": "^3.30.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.15.0",
    "eslint": "^9.12.0",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  }
}
